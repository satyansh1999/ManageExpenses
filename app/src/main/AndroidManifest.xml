<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="androidsamples.java.DigitalDiary">

  <application
      android:name="androidsamples.java.DigitalDiary.JournalApplication"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="DigitalDiary"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/Theme.JournalApp"
      android:fullBackupContent="@xml/backup_descriptor">
    <activity
        android:name="androidsamples.java.DigitalDiary.MainActivity"
        android:exported="true">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>